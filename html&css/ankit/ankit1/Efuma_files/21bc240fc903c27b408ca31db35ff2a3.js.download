 jQuery(document).ready(function(){
		        
				jQuery('.menu-toggle').click(function()
                {
                    jQuery('.mobile-menu > #nav').toggleClass('open');
                });
				jQuery('.mobile-menu').find('li').each(function(){
			if(jQuery(this).find('ul').length > 0){ 
			  jQuery(this).addClass('hasCategory');
			}
			jQuery(this).find('a').eq(0).click(function(e){
			if(jQuery(this).parent().find('ul').length <= 0){return }
			e.preventDefault();
			jQuery(this).parent().nextAll().each(function(){
				if(jQuery(this).hasClass('active')){
                    jQuery(this).find('ul').hide();
					jQuery(this).removeClass('active');
					
				}
				
			});
			jQuery(this).parent().prevAll().each(function(){
				if(jQuery(this).hasClass('active')){
					jQuery(this).find('ul').hide();
					jQuery(this).removeClass('active');
					

				}
				
			});
           
			/**************/
			
			if(jQuery(this).parent().find('ul').eq(0).is(':visible')){
				jQuery(this).parent().removeClass('active');
				jQuery(this).parent().find('ul').hide();
				
			}else{

				jQuery(this).parent().find('ul').eq(0).show();
				jQuery(this).parent().addClass('active');
                
			}
			});
		});
		if(jQuery(window).width() < 617){
           jQuery('.left-links').addClass('mobile-top');
		}
        jQuery('.top-menu-icon').click(function()
                {
                    jQuery('.mobile-top > ul').toggleClass('show');
                });
				
				jQuery('.mobile-info').click(function()
                {
                    jQuery('.header-info p').toggleClass('display');
                });

