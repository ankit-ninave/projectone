     jQuery(".product-image").mouseout(function(){
         jQuery(this).next('.short-desc').fadeOut('myTip');
     });*/

     var brdCrm = jQuery('.breadcrumbs').wrap('<div>').parent().html();
     jQuery('.cms-nordik-by-vapeson-2 .breadcrumbs').remove();
     jQuery('.cms-nordik-by-vapeson-2 .nordik-by-vapeson-2-wrapper').prepend(brdCrm);

