;(function() {
    jQuery.fn.oscpDrawer = function( options ) {
        var settings = jQuery.extend({
            targetElement: '',
            speed: 600
        }, options);

        drawerEle = jQuery(this);

        jQuery(settings.targetElement).click(function(){
            if(jQuery('#oscpDrawerMainContainer').length) return;

            jQuery('body').append('<div id="oscpDrawerMainContainer" style=""><div id="oscpDrawerMainContent"></div></div><div class="oscpDrawerOverlay"></div>');

            setMenuSkeleton('ltr');

            jQuery('body').css({
             height: jQuery(window).height()+'px',
             overflow: 'hidden',
             /*position: 'fixed',*/
                height: '100%',
                width: '100%'

             });
            jQuery('body').addClass('mob-onfixed');
            openDrawer();
        });

        var bp = { xsmall: 300, small: 767, medium: 1025 };
        var deviceWidth = 0;

        if(Modernizr.mq("screen and (max-width:" + bp.small + "px) and (min-width:" + bp.xsmall + "px)")){
            windwidth = bp.medium;
        } else if(Modernizr.mq("screen and (max-width:" + bp.xsmall + "px) ")){
            windwidth = bp.xsmall;
        }
        jQuery(window).resize(function(){
            if(Modernizr.mq("screen and (max-width:" + bp.small + "px) and (min-width:" + bp.xsmall + "px)")){
                deviceWidth = bp.medium;
            }else if(Modernizr.mq("screen and (max-width:" + bp.xsmall + "px) ")){
                deviceWidth = bp.xsmall;
            }
            jQuery('.oscpDrawerOverlay,.oscpDrawerContent').css({
                height: jQuery(window).height()+'px'
            });
            if(typeof(windwidth) == 'undefined'){
                var windwidth = jQuery(window).width();
            }
            /*if(windwidth != deviceWidth){
                windwidth = deviceWidth;
                jQuery('body').removeAttr('style');
                jQuery('#oscpDrawerMainContainer').remove();
                jQuery('.oscpDrawerOverlay').remove();
                jQuery('.oscpCloseDrawer').remove();
            }*/
            if (jQuery(window).width() > 1024) {
                closeDrawer()
            }
        });
        jQuery(document).on('click', '.drawerBack', function(){
            setMenuSkeleton('rtl', drawerEle.find('ul[rel="'+jQuery(this).attr('rel')+'"]'));
        });
        jQuery(document).on('click', '.opener', function(){
            setMenuSkeleton('ltr', drawerEle.find('ul[rel="'+jQuery(this).parent().parent().attr('rel')+'"]'));
        });

        jQuery(document).on('touchend', '.oscpDrawerOverlay', function(){
            closeDrawer();
        });

        jQuery(document).on('click', '.oscpDrawerOverlay', function(){
            closeDrawer();
        });

        jQuery(document).on('click', '.oscpCloseDrawer', function(){
            closeDrawer();
        });

        jQuery(document).on('touchend', '.oscpCloseDrawer', function(){
            closeDrawer();
        });

        function setMenuSkeleton(direction, ele) {
            //skeletonHtml = '<div style="opacity: 0;" class="oscpDrawerContent"><div class="container">'+getMenuItem(ele)+'<div style="padding: 15px;" class="form-search"><input id="search" name="q" class="input-text" placeholder="SEARCH" type="text"></div></div></div>';


            jQuery('.currentDrawer').removeClass('currentDrawer');
            var loginmenuheader = jQuery('.mob-loginshow').html();

            skeletonHtml = '<div class="oscpDrawerContent"><div class="innercontainer"><div id="oscpDrawerContainerHead"><div class="login-legal-mob">'+loginmenuheader+'</div></div>'+getMenuItem(ele)+'</div></div>';

            //jQuery('.oscpDrawerContent').eq(0).addClass('currentDrawer');
            if(direction == 'ltr'){
                jQuery('#oscpDrawerMainContent').prepend(skeletonHtml);
                jQuery('.oscpDrawerContent').eq(0).addClass('currentDrawer');
                setTimeout(function() {
                    jQuery('#oscpDrawerMainContent').addClass('highlight');
                }, 1000);
            }else{
                jQuery('#oscpDrawerMainContent').append(skeletonHtml);
                jQuery('.oscpDrawerContent').eq(0).addClass('currentDrawer');

            }



            containerWidth = jQuery('.oscpDrawerContent').width();
            containerLength = jQuery('.oscpDrawerContent').length;

            if(direction == 'ltr'){
                leftPos = containerWidth;
                leftPos = leftPos - (leftPos*2);
                jQuery('.oscpDrawerContent').eq(0).css({
                    left: (leftPos)+'px'
                });
            }else{
                jQuery('.oscpDrawerContent').eq(1).css({
                    left: '0px'
                });
            }

            jQuery('.oscpDrawerOverlay, .oscpDrawerContent').css({
                height: jQuery(window).height()+'px'
            });

            slideDrawer(direction);

        }

        function slideDrawer(direction) {
            containerWidth = jQuery('.oscpDrawerContent').eq(0).width();
            leftPos = containerWidth - (containerWidth*2);

            if(direction == 'ltr'){
                jQuery('.oscpDrawerContent').eq(0).delay(100).animate({opacity: '1',  left: '0px'}, settings.speed, function(){
                    jQuery('.oscpDrawerContent').eq(1).remove();
                });

                jQuery('.oscpDrawerContent').eq(1).find('.innercontainer').animate({/*opacity: '0'*/}, settings.speed);

            }else{
                jQuery('.oscpDrawerContent').eq(0).delay(100).animate({opacity: '1',  left: leftPos+'px'}, settings.speed, function(){
                    jQuery('.oscpDrawerContent').eq(0).remove();
                });
                jQuery('.oscpDrawerContent').eq(0).find('.innercontainer').animate({/*opacity: '0'*/}, settings.speed);
                /*jQuery('.oscpDrawerContent').eq(0).delay(50).animate({opacity: '1',left: (leftPos*2)+'px'}, settings.speed, function(){
                    jQuery('.oscpDrawerContent').eq(0).remove();
                })
                jQuery('.oscpDrawerContent').eq(1).delay(100).animate({opacity: '1',left: leftPos+'px'}, settings.speed)*/
            }
        }
        function getMenuItem(ele) {

            if((typeof(ele) == 'undefined') || (!ele.length) ){
                ele = drawerEle.find('ul').eq(0);
            }

            menuHtml = "<ul class='menuhtmlsec' rel='"+ele.attr('rel')+"'>";



            if(typeof(ele.attr('rel')) != 'undefined'){
                menuHtml += "<li class='drawerBackLi'><span rel='"+ele.parent().parent().attr('rel')+"' class='drawerBack'></span><a>"+(ele.parent().find('a').eq(0).text())+"</a><div class='oscpCloseDrawer'>X</div></li>";
            }else{
                menuHtml += "<li class='drawerBackLi'><a>Menu</a><div class='oscpCloseDrawer'>X</div></li>";
            }

            ele.children('li').each(function() {
                menuHtml += "<li rel='"+jQuery(this).attr('rel')+"' class='"+jQuery(this).attr('class')+"'><div class='mneu-anchor'><a href='"+jQuery(this).find('a').eq(0).attr('href')+"'>"+(jQuery(this).find('a').eq(0).html())+"</a></div>";
                menuHtml += (jQuery(this).find('ul').length) ? "<div class='openarrow' style='overflow: hidden;'><span class='opener'></span></div>" : "";
                menuHtml += "<div style='height: 1px; clear: left;'></div></li>";
            });

            menuHtml += "<ul>";

            return menuHtml;
