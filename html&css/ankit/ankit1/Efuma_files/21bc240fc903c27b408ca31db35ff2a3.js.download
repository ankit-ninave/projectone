jQuery(document).on("click",".more, .less",function(){
    var obj = jQuery(this);
    var currentQty = obj.siblings('.cart-item-quantity').val();
    var iid = obj.siblings('.cart-item-quantity').attr('data-item-id');

    if(obj.hasClass('more')){
        var newAdd = parseInt(currentQty)+parseInt(1);
        obj.siblings('#qinput-'+iid).val(newAdd);
        obj.siblings('#qinput-'+iid).attr('data-item-qty',newAdd);
        obj.siblings('#qbutton-'+iid).removeAttr('disabled');
        obj.siblings('#qbutton-'+iid).click();
    }
    if(obj.hasClass('less')){
        var newAdd = parseInt(currentQty)-parseInt(1);
        if(newAdd == 0 ){return;}
        obj.siblings('#qinput-'+iid).attr('value',newAdd);
        obj.siblings('#qinput-'+iid).attr('data-item-qty',newAdd);
        obj.siblings('#qbutton-'+iid).removeAttr('disabled');
        obj.siblings('#qbutton-'+iid).click();
    }
});

